  "description": "Compound components for the Web",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/compound-web.umd.cjs",
  "module": "./dist/compound-web.js",
  "exports": {
    ".": {
      "import": "./dist/compound-web.js",
      "require": "./dist/compound-web.umd.cjs",
      "types": "./dist/index.d.ts"
    },
