  "description": "Compound components for the Web",
  "type": "module",
  "main": "./dist/compound-web.js",
  "module": "./dist/compound-web.js",
  "exports": {
    ".": {
      "require": "./dist/compound-web.cjs",
      "import": "./dist/compound-web.js",
      "types": "./dist/index.d.ts",
      "style": "./dist/style.css"
