    "prettier:check": "npx prettier --check .",
    "lint": "yarn lint:js && yarn lint:ts && yarn lint:styles",
    "lint:js": "eslint --max-warnings=0 .",
    "lint:ts": "tsc --noEmit -p .",
    "lint:styles": "npx stylelint 'src/**/*.css'",
