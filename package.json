    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --max-warnings=0 .",
    "lint:styles": "npx stylelint \"**/*.css\"",
    "test": "jest",
    "prepercy": "node scripts/disableStoryStore7.js && yarn build-storybook",
